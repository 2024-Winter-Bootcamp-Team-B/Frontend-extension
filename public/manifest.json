{
  "manifest_version": 3,
  "name": "FOCUS",
  "version": "1.0.0",
  "description": "",
  "permissions": ["scripting", "sidePanel", "storage"],
  "host_permissions": ["https://*/*", "http://*/*"],
  "side_panel": {
    "default_path": "index.html"
  },
  "action": {
    "default_title": "Open Side Panel"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["http://localhost:5174/*"],
      "js": ["content.js"]
    }
  ]
}
